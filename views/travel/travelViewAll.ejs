<html>
<head>
    <title>Travel Agencies</title>
</head>
<body>
<h1>Travel Agencies</h1>
<a href="/travel/add">Add New Travel Agency</a> <br />

<br />

<table border="6">
    <tr><th>Email</th><th>Agency Name</th><th>Address</th><th>Action</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/travel/?email=<%= result[i].email%>"><%= result[i].email%></a></td>
        <td><%= result[i].agency_name%>
        <td><%= result[i].address%>

        <td>
            <button onclick="event.preventDefault();window.location.href='/travel/edit2/?email=<%= result[i].email %>'">Edit</button>
            <form action="/travel/delete/" style="display:inline;">
                <input type="hidden" name="email" id="email" value="<%= result[i].email %>" />
                <input type="submit" value="Delete"/>
            </form>
        </td>
    </tr>
    <% } %>
</table>
</body>

</html>